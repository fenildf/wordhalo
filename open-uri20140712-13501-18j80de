$(function (){
    //右侧dict_right5
    var iTab = $('.d_r5_tab li a');
    for(var i=0; i<iTab.length - 1; i++){
        iTab[i].index = i;
        iTab[i].onmouseover = function (){
            iTab.removeClass('cur');
            $('.d_r5_cont').hide();
            this.className = 'cur';
            $('.d_r5_cont').eq(this.index).show();
        };
    }

    //add by wangwenwu 2014-03-13, test
	
	//内容模块顶部吸附
	var oScroll = {};
	//oScroll.aJsContMod = $('.js_contMod');
	oScroll.aJsContMod = $('#dict_main > div').not($('.dictbar'));
	oScroll.aLeftLabel = $('.dict_nav ul:first li');
	oScroll.iScrollH = 0;	//滚动条滚动距离
	oScroll.iTop = 0;	////吸附元素顶部距离
	oScroll.iOldH = 0;	//吸附元素原始高度
	oScroll.iOldW = 0;	//吸附元素原始宽度
	oScroll.iViewH = document.documentElement.clientHeight; //可视区高度
	
	window.onscroll = function (){
		for(var i=0; i<oScroll.aJsContMod.length; i++){
			toFixed(oScroll.aJsContMod.eq(i),i);
		}	
	};
	
	function toFixed(obj,iIndex){
		oScroll.iScrollH = document.documentElement.scrollTop || document.body.scrollTop;
		oScroll.iTop = obj.offset().top;
		oScroll.iOldH = obj.height();
		oScroll.iOldW = obj.width();
		if(oScroll.iScrollH >= oScroll.iTop){
			obj.css({'padding-top':'57px'});
			obj.find('.tab_list').css({'position':'fixed','_position':'absolute','top':'0','z-index':'999','width':oScroll.iOldW,'background-color':'#FFF'});
			changeLeftLable(iIndex + 1);
		}else{
			obj.css({'padding-top':'15px'});
			obj.find('.tab_list').css({'position':'static','_position':'static'});
			if(iIndex == 0){
				changeLeftLable(iIndex);
			}
		}
		if((oScroll.iTop + oScroll.iOldH) - oScroll.iScrollH < oScroll.iViewH/4){
			obj.css({'padding-top':'15px'});
			obj.find('.tab_list').css({'position':'static','_position':'static'});
		}
	}
	function changeLeftLable(index){
		oScroll.aLeftLabel.removeClass('dict_nav_hover');
		oScroll.aLeftLabel.eq(index).addClass('dict_nav_hover');
	}
});
</script>

</body>

