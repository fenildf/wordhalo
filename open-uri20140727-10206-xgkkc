<script type="text/javascript">
function rollBar(obj,objP){
	var rollId =$(obj);
	var rollIdP=$(objP);
	$(window).scroll(function(){
		var rolloff = rollId.offset();
		var rolloffH =rollId.height();
		var rolloffP = rollIdP.offset();
		var rolloffPH =rollIdP.height()-205;
		var scrollTop = $(window).scrollTop();
		if($.browser.version<8){
			if(scrollTop>rolloffP.top){
				rollId.css({"position":"absolute","top":scrollTop-rolloffP.top});
				var objTop =scrollTop-rolloffP.top;
				var objMax =rolloffPH - rolloffH;
				if(objTop>objMax){
					rollId.css({"position":"absolute","top":objMax-5});
				}
			}else{
				rollId.css({"position":"absolute","top":-1});
			}
		}else{
			if(scrollTop>rolloffP.top){
				rollId.css({"position":"fixed","top":-1});
				var objTop =scrollTop-rolloffP.top;
				var objMax =rolloffPH - rolloffH;
				if(objTop>objMax){
					rollId.css({"position":"absolute","top":objMax-1});
				}
			}else{
				rollId.css({"position":"absolute","top":-1});
			}
		}
	})
}
function pendant(obj){
	var pendantId =$(obj);
	if($.browser.version<7){
		$(window).scroll(function(){
			var scrollTop = $(window).scrollTop();
			pendantId.css({"position":"absolute","top":scrollTop+80});
		})
	}
}
