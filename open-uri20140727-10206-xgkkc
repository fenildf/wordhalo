  <script>

function change(type){
	$.ajax({
		url: "./index.php?c=ajaxchange&type="+type,
		cache: false,
		type:"get",
		async:false,
		dataType: "json",
		success:function(ret){
			var myobj=eval(ret.code);
			var str= "";
			for(var i=0;i<myobj.length;i++){
				var worurl = myobj[i].word.replace( / /g, "_");
				if(i<10){
					str+="<li><a href='./"+worurl+"' title='"+myobj[i].word+"'>"+myobj[i].word+"</a></li>";
				}else{
					str+="<li style='display:none'><a href='./"+worurl+"' title='"+myobj[i].word+"'>"+myobj[i].word+"</a></li>";
				}
			}
			if(type == "new"){
				$("#word_new").html(str);
			}else if(type == "top"){
				$("#word_top").html(str);
			}

		}
	});
}


</script>

