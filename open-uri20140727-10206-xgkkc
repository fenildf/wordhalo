<script>
function _load_delay_img( b , c )
{
	var img = new Image(0,0);
	img.src = b;
	img.onload = img.onreadystatechange = function()
	{
		if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete")
		{
			img.onload = img.onreadystatechange = null;
			if (typeof c === "function")
			{
				c();
			}
		}
	};
}
var huaci_js = "http://open.iciba.com/huaci/huaci.js";
window.onload = function(){
	/*$c.settimeout( 100 , function()
	{
	$c._load_body_js(huaci_js,function()
	{
	$("#iciba_huayi").html(ICIBA_HUAYI_Str);

	});

	});*/
	setTimeout(function()
	{
		/*
		if( $('#top_ads_src').length > 0 ){
		_load_delay_img( $('#top_ads_src').attr('delay_src') , function(){
		$('#top_ads_src').attr( 'src' , $('#top_ads_src').attr('delay_src') );
		} );
		}
		if( $('#slider').length > 0 ){
		_load_delay_img( $('#slider').attr('delay_src') , function(){
		$('#slider').attr( 'src' , $('#slider').attr('delay_src') );
		$("div.slider").show();
		} );
		}
		*/
		$("div.slider").show();

		if( $('#top_ads_count').length > 0 ){
			_load_delay_img( $('#top_ads_count').attr('delay_src') , function(){
				$('#top_ads_count').attr( 'src' , $('#top_ads_count').attr('delay_src') );
			} );
		}
		
		//判断是否加入生词本
		if(ICIBA_TOP.user.id){
			var word_cur = location.pathname.substr(1);
			if(word_cur){
				$.ajax({
					url: "index.php?c=checkword",
					data: "word="+word_cur,
					type:"post",
					dataType: "json",
					success:function(ret){
						if(typeof ret == "object" && ret.errno == 2){
							$("a.join_word").addClass("cur");									
							$("a.join_word").attr("title", "已添加生词到默认生词本");									
						}
					}
				});
			}
		}

	}, 100);
};




document.domain = 'iciba.com';
var $c = (function()
{
	var that = {},_html,_htmltips,$d = document;
	that._load_js = function(b, c)
	{
		var a = document.createElement("script");
		a.setAttribute("type", "text/javascript");
		a.setAttribute("src", b);
		a.onload = a.onreadystatechange = function()
		{
			if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete")
			{
				a.onload = a.onreadystatechange = null;
				if (typeof c === "function")
				{
					c(b, true);
				}
			}
		};
		a.onerror = function(d)
		{
			if (typeof c === "function")
			{
				c(b, false);
			}
		};
		$d.getElementsByTagName("head")[0].appendChild(a);
	};
	that._load_body_js = function(b, c)
	{
		var a = document.createElement("script");
		a.setAttribute("type", "text/javascript");
		a.setAttribute("src", b);
		a.onload = a.onreadystatechange = function()
		{
			if (!this.readyState || this.readyState == "loaded" || this.readyState == "complete")
			{
				a.onload = a.onreadystatechange = null;
				if (typeof c === "function")
				{
					c(b, true);
				}
			}
		};
		a.onerror = function(d)
		{
			if (typeof c === "function")
			{
				c(b, false);
			}
		};
		$d.getElementsByTagName("body")[0].appendChild(a);
	};
	that.settimeout = function( t , f )
	{
		if(typeof(f)=="function")
		{
			setTimeout(function()
			{
				f()
			}, t);
		}
	};
	return that;
})();

</script>
