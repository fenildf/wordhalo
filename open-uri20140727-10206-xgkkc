<script>
function pop_clickcount( i ){
	var i0  = new Image(0,0);
	counter = 'http://goto.www.iciba.com/3b87652ba0916c03c634d5db8558d494.php';
	i0.src = counter+"?"+"sid=22&pid=2872&spid=0&loid=0&aid=8694&liid="+i+"&pvid=0&uid=0&rnum="+Math.floor(Math.random());
}
</script>


<script>

/*
