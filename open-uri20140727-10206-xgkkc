$(".dict_nav_his_m").hover(function(){
	$(".dict_nav").css("z-index","1000");
	$(".dict_nav_his").css("border-color","#e6e6e6");
	var h=$(".dict_nav_his").height()-35;
	$(".dict_nav_his_l").css({"display":"block","height":h});},
	function(){
		$(".dict_nav_his_l").css("display","none");
		$(".dict_nav_his").css("border-color","#fff");
		$(".dict_nav").css("z-index","auto")
	}
	);
	$(".pendant_w").hover(function(){
		$(".pendant_wx").css("display","block");
	},
	function(){
		$(".pendant_wx").css("display","none");
	}
	);
	$("#vMobile").hover(function(){
		$("#vMobile_box").show();
	},function(){
		$("#vMobile_box").hide();
	}
